 </mstab>



 <wing x="-1.2" y="0.9" z="0"
chord="3.1" 
length="3.3"
taper="0.30" 
incidence="0"
sweep="13" 
dihedral="-0.7"
camber="0.0">
  <stall aoa="18" width="2" peak="1.3"/>
  <flap0 start="0.01" end="0.95" lift="1.5" drag="1.7"/>
  <slat start="0.1" end="0.8" aoa="30" drag="1.0"/>
  <flap1 start="0.1" end="0.69" lift="1.2" drag="1.4"/>
