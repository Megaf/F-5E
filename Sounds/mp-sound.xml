   <engine-wake>
      <name>Engine Wake</name>
      <mode>looped</mode>
      <path>wake.wav</path>
      <condition>
        <not><property>sim/multiplay/generic/int[7]</property></not>
      </condition>
      <position>
        <x>5</x>
        <y>0</y>
        <z>-0.25</z>
      </position>
      <orientation>
        <x>1</x>
        <y>0</y>
        <z>0</z>
        <inner-angle>30.0</inner-angle>
        <outer-angle>90.0</outer-angle>
        <outer-gain>0.2</outer-gain>
      </orientation>
      <volume>
        <property>engines/engine/n1</property>
        <factor>0.1</factor>
        <offset>-7.0</offset>
        <min>0.00</min>
        <max>10</max>
      </volume>
      <volume>
        <property>velocities/groundspeed-kt</property>
        <factor>0.0175</factor>
        <offset>0.25</offset>
        <min>0.00</min>
        <max>1.1</max>
      </volume>
      <reference-dist>80</reference-dist>
      <max-dist>12000</max-dist>
    </engine-wake> 
  
